{
  "name": "Генератор тестовых данных",
  "description": "Генератор тестовых данных",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "content_scripts": {
      "js": [
        "generators/*"
      ]
    },
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "commands": {
    "snils": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Вставить СНИЛС",
      "global": true
    },
    "innIndividual": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "Вставить ИНН ФЛ",
      "global": true
    },
    "innLegal": {
      "suggested_key": {
        "default": "Ctrl+Shift+3",
        "mac": "Command+Shift+3"
      },
      "description": "Вставить ИНН ЮЛ",
      "global": true
    },
    "defaultPassword": {
      "suggested_key": {
        "default": "Ctrl+Shift+4",
        "mac": "Command+Shift+4"
      },
      "description": "Вставить Стандартный пароль",
      "global": true
    }
  }
}