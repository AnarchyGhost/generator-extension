{
  "name": "Генератор тестовых данных",
  "description": "Генератор тестовых данных",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "content_scripts": {
     "js": ["generators/*"]
    },
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}